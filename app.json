{
  "expo": {
    "name": "Vårt Landskrona",
    "slug": "vart-landskrona",
    "version": "1.0.0",
    "owner": "landskrona-kommun",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.landskrona.vartlandskrona",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Denna app behöver åtkomst till kameran för att kunna ladda upp bilder till ärenden.",
        "NSLocationWhenInUseUsageDescription": "Denna app behöver åtkomst till din plats för att visa din plats på karta.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "se.landskrona.VartLandskrona"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a3960895-02ad-4b6f-b315-253739b09df0"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0e5873",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ],
      "expo-asset"
    ]
  }
}
