{
  "expo": {
    "name": "Vårt Landskrona",
    "slug": "vart-landskrona",
    "version": "2.0.4",
    "owner": "landskrona",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.landskrona.vartlandskrona",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Denna app behöver åtkomst till kameran för att kunna ladda upp bilder till ärenden.",
        "NSLocationWhenInUseUsageDescription": "Denna app behöver åtkomst till din plats för att visa din plats på karta."
      }
    },
    "android": {
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#134F63"
      },
      "package": "se.landskrona.VartLandskrona"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "db5f87e0-9ae7-49af-a7c2-a9da90f0db27"
      }
    },
    "plugins": ["expo-font",
        ["expo-splash-screen",
        {
          "backgroundColor": "#134F63",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ]
    ]
  }
}
