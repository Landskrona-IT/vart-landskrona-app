{
  "cli": {
    "version": ">= 3.14.0",
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_WEBVIEW_URL": "https://helsingborg-stad.github.io/ett-battre-helsingborg-web/",
        "EXPO_PUBLIC_DISTURBANCE_API": "https://alarm.helsingborg.io/json/wp/v2/disturbances/?place=2"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_WEBVIEW_URL": "https://helsingborg-stad.github.io/ett-battre-helsingborg-web/",
        "EXPO_PUBLIC_DISTURBANCE_API": "https://alarm.helsingborg.io/json/wp/v2/disturbances/?place=2"
      }
    },
    "production": {
      "env" : {
        "EXPO_PUBLIC_WEBVIEW_URL": "https://helsingborg-stad.github.io/ett-battre-helsingborg-web/",
        "EXPO_PUBLIC_DISTURBANCE_API": "https://alarm.helsingborg.io/json/wp/v2/disturbances/?place=2"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
